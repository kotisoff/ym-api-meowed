{
  "name": "ym-api-meowed",
  "version": "1.3.6",
  "description": "A Node.js wrapper for the Yandex.Music API (Unofficial) http://music.yandex.ru",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Anton Petrov",
  "contributors": [
    "Anton Petrov (https://itsmepetrov.com)",
    "Evgeny Kontsevoy (https://github.com/kontsevoye)",
    "Donnevtis (https://github.com/Donnevtis)",
    "KotIsOff (https://github.com/kotisoff)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kotisoff/ym-api-meowed.git"
  },
  "keywords": [
    "yandex",
    "music",
    "api",
    "wrapper",
    "yandex.music"
  ],
  "dependencies": {
    "axios": "^1.11.0",
    "fast-xml-parser": "^5.2.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "dotenv-cli": "^10.0.0",
    "prettier": "3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "tsc",
    "typecheck:examples": "tsc -p tsconfig.examples.json",
    "prepublish": "npm run build",
    "format": "prettier --write .",
    "smoke:xml": "tsx example/smoke-xml-parse.ts",
    "smoke:token": "dotenv -e .env -- tsx example/token.ts",
    "smoke:artist": "dotenv -e .env -- tsx example/artist.ts",
    "smoke:album": "dotenv -e .env -- tsx example/album.ts",
    "smoke:feed": "dotenv -e .env -- tsx example/feed.ts",
    "smoke:genres": "dotenv -e .env -- tsx example/genres.ts",
    "smoke:playlist": "dotenv -e .env -- tsx example/playlist.ts",
    "smoke:playlist-share": "dotenv -e .env -- tsx example/playlist-share.ts",
    "smoke:track": "dotenv -e .env -- tsx example/track.ts",
    "raw:track": "dotenv -e .env -- tsx example/raw-track.ts",
    "smoke:get-track": "dotenv -e .env -- tsx example/get-track.ts",
    "smoke:tracks": "dotenv -e .env -- tsx example/tracks.ts",
    "smoke:track-wrapped": "dotenv -e .env -- tsx example/track-wrapped.ts",
    "smoke:summary": "tsx smoke-runner.ts",
    "smoke:station-new": "dotenv -e .env -- tsx example/station-new.ts",
    "smoke:station-tracks": "dotenv -e .env -- tsx example/station-tracks.ts",
    "smoke:create-rotor-session": "dotenv -e .env -- tsx example/create-rotor-session.ts",
    "smoke:session-tracks": "dotenv -e .env -- tsx example/session-tracks.ts"
  }
}
